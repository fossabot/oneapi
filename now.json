{
  "version": 2,
  "name": "oneapi",
  "builds": [
    { "src": "endpoints/*.py", "use": "@now/python" },
    { "src": "endpoints/*.ts", "use": "@now/node" }
  ],
  "routes": [
    { "src": "/api/v0/name", "dest": "/endpoints/name.py" },
    { "src": "/api/v0/(.*)", "dest": "/endpoints/$1.ts" }
  ]
}
